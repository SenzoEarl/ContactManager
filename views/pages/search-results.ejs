<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <div class="jumbotron">
        <h1>Contacts</h1>
        <p>These are all your search results</p>
        <ul>
            <% Object.keys(result).forEach(function(contact) { %>
                <li> <%= contact.name%> <span>Cell: <%= contact.cell %></span></li>
            <% }); %>
        </ul>
    </div>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>